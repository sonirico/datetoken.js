{
  "name": "datetoken",
  "version": "0.1.0",
  "description": "Parse relative datetime tokens into date objects",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "exit 0; tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "rm -rf dist/ && npm run build && TZ='UTC' npx mocha test",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonirico/datetoken.js.git"
  },
  "keywords": [
    "relative",
    "date",
    "token",
    "presets",
    "datetime",
    "parser",
    "now",
    "yesterday",
    "last week"
  ],
  "author": "sonirico",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonirico/datetoken.js/issues"
  },
  "homepage": "https://github.com/sonirico/datetoken.js#readme",
  "dependencies": {
    "date-fns": "^1.29.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "prettier": "^1.14.3",
    "sinon": "^7.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  }
}
